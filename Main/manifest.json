{
    "name": "Getting Started Example",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "escapetherabbithole.html"
    },
    "content_security_policy": {
      "extension_pages": "script-src 'self' https://www.googleapis.com/;"
    },
    "permissions": ["activeTab"],
    "commands": {
        "_execute_action": {
          "suggested_key": {
            "default": "Ctrl+Shift+F",
            "mac": "MacCtrl+Shift+F"
          },
          "description": "Opens hello.html"
        }
      }
}






    

